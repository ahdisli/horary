{
  "info": {
    "name": "Realtime Token Test",
    "description": "Test collection for OpenAI Realtime Token generation",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get Realtime Token - Local",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{SUPABASE_ANON_KEY}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"session\": {\n    \"instructions\": \"You are a helpful horary astrologer assistant.\"\n  }\n}"
        },
        "url": {
          "raw": "{{BASE_URL}}/functions/v1/realtime-token",
          "host": ["{{BASE_URL}}"],
          "path": ["functions", "v1", "realtime-token"]
        }
      },
      "response": []
    },
    {
      "name": "Get Realtime Token - Default Config",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{SUPABASE_ANON_KEY}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{}"
        },
        "url": {
          "raw": "{{BASE_URL}}/functions/v1/realtime-token",
          "host": ["{{BASE_URL}}"],
          "path": ["functions", "v1", "realtime-token"]
        }
      },
      "response": []
    }
  ],
  "variable": [
    {
      "key": "BASE_URL",
      "value": "http://127.0.0.1:54321"
    },
    {
      "key": "SUPABASE_ANON_KEY",
      "value": "dummy-key"
    }
  ]
}
